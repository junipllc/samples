{#
/**
 * @file
 * Theme override for the "More CLIENTNAME [sport]" block.
 *
 * Available variables:
 * - content: contains the link list.
 */
#}

<div{{ attributes }}>
  {{ title_prefix }}
  {{ title_suffix }}
  {% block content %}
    {% include '@organisms/link-stack-block/_link-stack-block.twig' with {
      link_stack_block: {
        links: link_list,
        title: title,
        classes: link_stack_classes,
        no_padding: no_padding,
        title_alter: title_alter,
      }
    }%}
  {% endblock %}
</div>
